<a href="<%= nameID %>/add">add new item</a>


<p>All from <%= title %></p>

        
        
<div class="post">
<% for(let document of documents) {%>
    
  <a href="http://localhost:3000/admin/posts/<%= document._id %>">  
    <% document = document.toObject()%>
    <% for(let key in document) {%>
        
        <% if (key === "_id") continue; %>
        <% if (key === "rewrite" ) continue;%>
        <% let field = document[key]; %>
        <% let fieldType = document.rewrite.nameID2fieldType[key]; %>
        <% let rewrite = document.rewrite.nameID2name[key] %>
       
        <% if(rewrite) {%>
            <h3> <%= rewrite %>: </h3> 
        <% } else { %> 
            <h3><%= key %>:</h3> 
        <% } %> 

   

        <%if(fieldType === "image"){%>
            <img src="<%= "uploads/" + document[key].filename  %>"/>
        <%} else {%> 
            <!-- admin/uploads/ -->
         
            <%- document[key] %>
        <%}%> 
        <br>
    <% } %> 
    <br><br>
</a>
<% } %> 
</div>
